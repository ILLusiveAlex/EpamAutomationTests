<?xml version="1.0" encoding="utf-8"?>
<RunSettings>
  <TestRunParameters>
    <Parameter name="Browser" value="chrome" />
    <Parameter name="BaseUrl" value="https://www.epam.com" />
    <Parameter name="ImplicitWait" value="30" />
    <Parameter name="PageLoadTimeout" value="30" />
    <Parameter name="ScriptTimeout" value="30" />
  </TestRunParameters>
  <RunConfiguration>
    <ResultsDirectory>TestResults</ResultsDirectory>
    <MaxCpuCount>1</MaxCpuCount>
    <TestSessionTimeout>600000</TestSessionTimeout>
  </RunConfiguration>
  <MSTest>
    <Parallelize>
      <Workers>1</Workers>
      <Scope>ClassLevel</Scope>
    </Parallelize>
  </MSTest>
</RunSettings>
